{
  "kind": "collectionType",
  "collectionName": "subscribe_blogs",
  "info": {
    "singularName": "subscribe-blog",
    "pluralName": "subscribe-blogs",
    "displayName": "Subscribe Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "maxLength": 200,
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "detail": {
      "type": "blocks"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "subscribe_blogs"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "subscribe_blogs"
    },
    "price": {
      "type": "string"
    }
  }
}
